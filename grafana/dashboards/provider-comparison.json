        {"expr": "sum(rate(rag_tokens_total[5m])) by (provider)", "format": "table", "instant": true, "refId": "G"},
        {"expr": "sum(rate(rag_errors_total[5m])) by (provider) / sum(rate(rag_requests_total[5m])) by (provider)", "format": "table", "instant": true, "refId": "H"}
