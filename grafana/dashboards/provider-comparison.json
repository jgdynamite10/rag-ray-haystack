        "defaults": {
          "color": {"mode": "thresholds"},
          "custom": {"align": "center", "cellOptions": {"type": "auto"}, "inspect": false},
          "mappings": [],
          "thresholds": {
            "mode": "absolute",
            "steps": [
              {"color": "green", "value": null},
              {"color": "yellow", "value": 0.5},
              {"color": "red", "value": 1}
            ]
          },
          "unit": "s"
        },
        "overrides": [
          {"matcher": {"id": "byName", "options": "Provider"}, "properties": [{"id": "custom.width", "value": 120}]},
          {"matcher": {"id": "byName", "options": "Requests/s"}, "properties": [{"id": "unit", "value": "reqps"}]},
          {"matcher": {"id": "byName", "options": "Tokens/s"}, "properties": [{"id": "unit", "value": "short"}]},
          {"matcher": {"id": "byName", "options": "Error %"}, "properties": [{"id": "unit", "value": "percentunit"}]}
