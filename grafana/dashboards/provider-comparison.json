      },
      "gridPos": {"h": 8, "w": 24, "x": 0, "y": 0},
      "id": 1,
      "options": {
        "cellHeight": "sm",
        "footer": {"countRows": false, "fields": "", "reducer": ["sum"], "show": false},
        "showHeader": true,
        "sortBy": [{"desc": false, "displayName": "TTFT p95"}]
      },
      "title": "Provider Comparison - Key Metrics",
      "type": "table",
      "transformations": [
        {"id": "merge", "options": {}},
        {"id": "organize", "options": {
          "excludeByName": {},
          "indexByName": {},
          "renameByName": {
            "Value #A": "Requests/s",
            "Value #B": "TTFT p50",
            "Value #C": "TTFT p95",
            "Value #D": "TPOT p50",
            "Value #E": "TPOT p95",
            "Value #F": "Latency p95",
            "Value #G": "Tokens/s",
            "Value #H": "Error %",
            "provider": "Provider"
          }
        }}
      ],
      "targets": [
        {"expr": "sum(rate(rag_requests_total[5m])) by (provider)", "format": "table", "instant": true, "refId": "A"},
        {"expr": "histogram_quantile(0.50, sum(rate(rag_ttft_seconds_bucket[5m])) by (le, provider))", "format": "table", "instant": true, "refId": "B"},
        {"expr": "histogram_quantile(0.95, sum(rate(rag_ttft_seconds_bucket[5m])) by (le, provider))", "format": "table", "instant": true, "refId": "C"},
        {"expr": "histogram_quantile(0.50, sum(rate(rag_tpot_seconds_bucket[5m])) by (le, provider))", "format": "table", "instant": true, "refId": "D"},
